{
  "version": 2,
  "builds": [
    {
      "src": "MyApi/wsgi.py",
      "use": "@vercel/python@3.1.0",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.10",
        "installCommand": "pip install -r requirements.txt"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "MyApi/wsgi.py"
    }
  ]
}
